{
  "name": "@mcph/miix-cli",
  "version": "0.1.1",
  "description": "Command line tool for developing with Mixer Interactive",
  "main": "dist/src/index.js",
  "bin": {
    "miix": "./bin/miix"
  },
  "scripts": {
    "test": "npm-run-all --parallel --silent test:lint test:unit fmt",
    "test:ci": "npm-run-all --parallel --silent test:lint test:cover",
    "test:cover": "nyc mocha",
    "test:unit": "mocha",
    "test:lint": "tslint --type-check --project tsconfig.json '{src,test}/**/*.ts'",
    "fmt": "npm-run-all --parallel --silent fmt:ts fmt:js && npm run test:lint -- --fix",
    "fmt:js": "prettier --single-quote --trailing-comma es5 --write --print-width 100 \"src/**/*.js\" \"bin/*\"",
    "fmt:ts": "prettier --single-quote --trailing-comma all --parser typescript --print-width 100 --write \"{src,test}/**/*.ts\"",
    "build": "rimraf dist && npm-run-all --parallel --silent build:ts build:editor",
    "build:ts": "tsc",
    "build:editor": "webpack --config src/ui/editor/webpack.config.js",
    "app": "tsc && npm run -s build:editor && electron bin/electron",
    "app:debug": "npm run -s app -- --debug",
    "prepare": "npm run -s build"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "!src/webpack/editor/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mixer/miix-cli.git"
  },
  "keywords": [
    "mixer",
    "interactive",
    "cli"
  ],
  "author": "Connor Peet <connor@xbox.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mixer/miix-cli/issues"
  },
  "homepage": "https://github.com/mixer/miix-cli#readme",
  "devDependencies": {
    "@angular/animations": "^4.3.4",
    "@angular/cdk": "^2.0.0-beta.12",
    "@angular/common": "^4.3.4",
    "@angular/compiler": "^4.3.4",
    "@angular/core": "^4.3.4",
    "@angular/forms": "^4.3.4",
    "@angular/http": "^4.3.4",
    "@angular/material": "^2.0.0-beta.12",
    "@angular/platform-browser": "^4.3.4",
    "@angular/platform-browser-dynamic": "^4.3.4",
    "@mcph/miix-participant": "^0.1.3",
    "@ngrx/store": "^4.0.2",
    "@types/chai": "^4.0.3",
    "@types/chai-as-promised": "0.0.31",
    "@types/chai-subset": "^1.3.0",
    "@types/codemirror": "0.0.44",
    "@types/express": "^4.0.36",
    "@types/form-data": "^2.2.0",
    "@types/glob": "^5.0.30",
    "@types/joi": "^10.4.1",
    "@types/js-yaml": "^3.9.0",
    "@types/json5": "0.0.29",
    "@types/lodash": "^4.14.74",
    "@types/marked": "^0.3.0",
    "@types/mocha": "^2.2.41",
    "@types/moment": "^2.13.0",
    "@types/node": "^8.0.50",
    "@types/node-fetch": "^1.6.7",
    "@types/ora": "^1.3.1",
    "@types/screenfull": "^3.3.1",
    "@types/sinon": "^2.3.3",
    "@types/sinon-chai": "^2.7.28",
    "@types/tar": "^4.0.0",
    "@types/yargs": "^8.0.2",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.2.2",
    "bluefill": "^0.1.3",
    "chai": "^4.1.1",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.5.0",
    "codemirror": "^5.28.0",
    "core-js": "^2.5.0",
    "css-loader": "^0.28.4",
    "electron": "^1.7.9",
    "eventemitter3": "^2.0.3",
    "fast-json-patch": "^2.0.4",
    "mocha": "^3.5.0",
    "moment": "^2.18.1",
    "node-sass": "^4.5.3",
    "normalize.css": "^7.0.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^11.2.1",
    "prettier": "^1.5.3",
    "raw-loader": "^0.5.1",
